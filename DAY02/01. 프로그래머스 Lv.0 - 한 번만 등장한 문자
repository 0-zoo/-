# https://school.programmers.co.kr/learn/courses/30/lessons/120896

from collections import defaultdict

def solution(s):
    answer = []
    cnt = defaultdict(int)
    # cnt = {}

    for ch in s:
        cnt[ch] += 1


    for key in cnt:
        if cnt[key] == 1:
            answer += key
    
    return "".join(sorted(answer))
